{
  "name": "batch",
  "version": "1.0.0",
  "description": "Batch processing for Facility Management System",
  "main": "dist/monthly-report.js",
  "scripts": {
    "build": "tsc",
    "start:monthly": "node dist/monthly-report.js",
    "start:annual": "node dist/annual-report.js",
    "dev:monthly": "ts-node src/monthly-report.ts",
    "dev:annual": "ts-node src/annual-report.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "pg": "^8.11.3",
    "aws-sdk": "^2.1490.0",
    "dotenv": "^16.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@types/yargs": "^17.0.32",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
